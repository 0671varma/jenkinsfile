pipeline{
agent any
stages 
{
stage('clone') 
{
steps{
echo "clone the Project.........."
sudo git clone https://github.com/0671varma/Food-App.git
}
}
stage('Build') 
{
steps{
echo "Building the Project.........."
}
}
stage('Test') 
{
steps{
echo "Testing the Project.........."
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
sudo yum -y install httpd
sudo mv Food-App/* /var/www/html/
sudo systemctl start httpd

}
}
}
}
